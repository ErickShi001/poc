<mvc:View controllerName="help.controller.List" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Page showNavButton="false" title="Order List">
		<headerContent>
			<Button icon="sap-icon://add" press="onNew" />
		</headerContent>
		<List id="idList" items="{/ORDRCollection}">
			<items>
				<StandardListItem title="Document ID {id}" description="{bpId}" type="Navigation" press="onPress" />
			</items>
		</List>

	</Page>
</mvc:View>